DBHandlerTest	test_dbhandler.php	/^class DBHandlerTest extends TestUnit {$/;"	c
TestUnit	TestUnit.php	/^class TestUnit {$/;"	c
__construct	test_dbhandler.php	/^	function __construct() {$/;"	f
assert_equal	TestUnit.php	/^	protected function assert_equal($val, $val2) {$/;"	f
dbh	test_helper.php	/^$dbh = new DBHandler();$/;"	v
fail	TestUnit.php	/^	private function fail($expr) {$/;"	f
rows	test_dbhandler.php	/^		$rows = $this->dbh->fetch_rows();$/;"	v
table_sql	test_helper.php	/^$table_sql = array();$/;"	v
test	test_dbhandler.php	/^$test = new DBHandlerTest();$/;"	v
